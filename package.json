{
  "name": "cmujmc",
  "private": true,
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "prebuild": "prisma generate && prisma migrate deploy",
    "build:1-next": "cross-env NODE_ENV=production next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "next-build": "next build",
    "dev:wss": "cross-env PORT=3001 tsx watch src/server/wssDevServer.ts --tsconfig tsconfig.server.json ",
    "dev:next": "next dev --turbopack",
    "dev": "pnpm migrate-dev && run-p dev:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "start": "cross-env NODE_ENV=production node dist/server/prodServer.js",
    "studio": "prisma studio",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" src",
    "lint-fix": "pnpm lint --fix",
    "test-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
    "test-start": "cross-env APP_ENV=test WS_URL=ws://localhost:3000 start-server-and-test start 3000 test",
    "test": "playwright test",
    "postinstall": "prisma generate",
    "promote": "ts-node src/promote_admin.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^4.1.3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.5.0",
    "@tanstack/react-query": "^5.59.16",
    "@trpc/client": "11.0.0-rc.601",
    "@trpc/next": "11.0.0-rc.601",
    "@trpc/react-query": "11.0.0-rc.601",
    "@trpc/server": "11.0.0-rc.601",
    "@valkey/valkey-glide": "^1.3.1",
    "cache-manager": "^6.4.2",
    "cacheable": "^1.8.3",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "corepack": "^0.31.0",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.5.0",
    "fuse.js": "^7.0.0",
    "immutable": "^5.0.3",
    "lodash": "^4.17.21",
    "next": "15.1.7",
    "next-auth": "^4.24.11",
    "next-intl": "^3.26.4",
    "protobufjs": "^7.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-svg": "^16.3.0",
    "react-use": "^17.6.0",
    "superjson": "^1.13.3",
    "tailwind-merge": "^2.5.4",
    "tsx": "^4.19.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@next/eslint-plugin-next": "^15.2.0",
    "@playwright/test": "^1.47.0",
    "@tanstack/react-query-devtools": "^5.55.4",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.16.5",
    "@types/react": "^19.0.10",
    "@types/ws": "^8.5.12",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^2.8.8",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "packageManager": "pnpm@10.6.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "protobufjs"
    ]
  }
}
